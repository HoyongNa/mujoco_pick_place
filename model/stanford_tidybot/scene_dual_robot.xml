<mujoco model="tidybot 4-room scene - dual robot">
  <!-- Include both robots -->
  <compiler angle="radian" meshdir="assets"/>
  
  <include file="tidybot_no_robocasa.xml"/>
  <include file="tidybot_robot2_no_robocasa.xml"/>
  
  <option timestep="0.002" solver="Newton" iterations="100" noslip_iterations="40" tolerance="1e-10" ls_tolerance="0.01"/>

  <!-- 더 큰 환경에 맞게 통계 조정 -->
  <statistic center="0 0 1" extent="6" meansize="0.1"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="140" elevation="-20"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
             markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="20 20" reflectance="0.2"/>
    
    <!-- 벽 재질 -->
    <material name="wall_material" rgba="0.85 0.85 0.85 1" reflectance="0.1"/>
    <material name="interior_wall" rgba="0.75 0.75 0.75 1" reflectance="0.1"/>
  </asset>

  <worldbody>
    <!-- 조명 - 각 방에 하나씩 -->
    <light pos="-2.5 2.5 3" directional="false" diffuse="0.8 0.8 0.8"/>
    <light pos="2.5 2.5 3" directional="false" diffuse="0.8 0.8 0.8"/>
    <light pos="-2.5 -2.5 3" directional="false" diffuse="0.8 0.8 0.8"/>
    <light pos="2.5 -2.5 3" directional="false" diffuse="0.8 0.8 0.8"/>
    
    <!-- 10m x 10m 바닥 -->
    <geom name="floor" type="box" size="5 5 0.05" pos="0 0 -0.05" material="groundplane"
          contype="1" conaffinity="1" condim="3"
          friction="1 0.005 0.0001" solimp="0.9 0.95 0.002" solref="0.001 1"
          margin="0.001" priority="1"/>

    <!-- ================ 외벽 (높이 2m) ================ -->
    <geom name="wall_north" type="box" size="5.1 0.1 1.05" pos="0 5 0.95" material="wall_material"/>
    <geom name="wall_south" type="box" size="5.1 0.1 1.05" pos="0 -5 0.95" material="wall_material"/>
    <geom name="wall_east" type="box" size="0.1 5 1.05" pos="5 0 0.95" material="wall_material"/>
    <geom name="wall_west" type="box" size="0.1 5 1.05" pos="-5 0 0.95" material="wall_material"/>

    <!-- ================ 내부 벽 (십자 구조) ================ -->
    <geom name="interior_horizontal_1" type="box" size="1.8 0.05 1.05" pos="-3.2 0 0.95" material="interior_wall"/>
    <geom name="interior_horizontal_2" type="box" size="1.8 0.05 1.05" pos="3.2 0 0.95" material="interior_wall"/>
    <geom name="interior_vertical_1" type="box" size="0.05 1.8 1.05" pos="0 3.2 0.95" material="interior_wall"/>
    <geom name="interior_vertical_2" type="box" size="0.05 1.8 1.05" pos="0 -3.2 0.95" material="interior_wall"/>

    <!-- ================ 방 1 (북서쪽) - Objects ================ -->
    <body name="red_box" pos="-2.7 2.5 0.035">
      <joint type="free"/>
      <geom type="box" size="0.035 0.035 0.035" rgba="1 0 0 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <body name="blue_box" pos="-2.4 2.5 0.035">
      <joint type="free"/>
      <geom type="box" size="0.035 0.035 0.035" rgba="0 0 1 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <!-- ================ 방 2 (북동쪽) - Objects ================ -->
    <body name="green_box" pos="2.3 2.5 0.035">
      <joint type="free"/>
      <geom type="box" size="0.035 0.035 0.035" rgba="0 1 0 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <body name="yellow_box" pos="2.7 2.5 0.05">
      <joint type="free"/>
      <geom type="box" size="0.05 0.05 0.05" rgba="1 1 0 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <!-- ================ 방 3 (남서쪽) - Objects ================ -->
    <body name="orange_box" pos="-2.7 -2.5 0.035">
      <joint type="free"/>
      <geom type="box" size="0.035 0.035 0.035" rgba="1 0.5 0 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <body name="purple_box" pos="-2.3 -2.5 0.05">
      <joint type="free"/>
      <geom type="box" size="0.05 0.05 0.05" rgba="0.5 0 0.5 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <!-- ================ 방 4 (남동쪽) - Objects ================ -->
    <body name="cyan_box" pos="2.3 -2.5 0.035">
      <joint type="free"/>
      <geom type="box" size="0.035 0.035 0.035" rgba="0 1 1 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <body name="pink_box" pos="2.7 -2.5 0.05">
      <joint type="free"/>
      <geom type="box" size="0.05 0.05 0.05" rgba="1 0.5 0.8 1" mass="0.5" friction="2 2 2" solref="0.01 1" solimp="0.9 0.95 0.002"/>
    </body>

    <!-- ================ 문 입구 장애물 벽 ================ -->
    <!-- <geom name="obstacle_horizontal_left" type="box" size="0.1 0.05 0.5" pos="-0.9 0.3 0.5" rgba="0.9 0.7 0.1 1" contype="1" conaffinity="1"/>
    <geom name="obstacle_horizontal_right" type="box" size="0.1 0.05 0.5" pos="0.9 0.3 0.5" rgba="0.9 0.7 0.1 1" contype="1" conaffinity="1"/>
    <geom name="obstacle_vertical_top" type="box" size="0.05 0.1 0.5" pos="0.3 0.9 0.5" rgba="0.9 0.7 0.1 1" contype="1" conaffinity="1"/>
    <geom name="obstacle_vertical_bottom" type="box" size="0.05 0.1 0.5" pos="0.3 -0.9 0.5" rgba="0.9 0.7 0.1 1" contype="1" conaffinity="1"/> -->
  </worldbody>
  
